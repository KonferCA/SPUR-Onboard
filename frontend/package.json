{
    "name": "spur",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "biome lint .",
        "lint:watch": "onchange -v 'src/**/*.ts' 'src/**/*.tsx' -- biome lint .",
        "lint:fix": "biome lint --write .",
        "format": "biome format .",
        "format:fix": "biome format --write .",
        "test": "vitest",
        "test:ci": "vitest run",
        "coverage": "vitest run --coverage",
        "preview": "vite preview",
        "typecheck:watch": "onchange 'src/**/*.ts' 'src/**/*.tsx' -- tsc -b"
    },
    "dependencies": {
        "@headlessui/react": "^2.2.0",
        "@mysten/sui": "^1.20.0",
        "@radix-ui/react-dialog": "^1.1.6",
        "@suiet/wallet-kit": "^0.3.3",
        "@tanstack/react-query": "4",
        "@tanstack/react-router": "^1.97.1",
        "@tanstack/react-table": "^8.20.6",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "framer-motion": "^11.18.1",
        "lucide-react": "^0.476.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.4.0",
        "tailwind-merge": "^3.0.1",
        "tailwindcss-animate": "^1.0.7",
        "vaul": "^1.1.2",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@tanstack/router-devtools": "^1.97.1",
        "@tanstack/router-plugin": "^1.97.1",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@testing-library/user-event": "^14.6.0",
        "@types/node": "^22.10.7",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "2.1.4",
        "autoprefixer": "^10.4.20",
        "globals": "^15.14.0",
        "jsdom": "^25.0.1",
        "onchange": "^7.1.0",
        "postcss": "^8.5.1",
        "prettier": "^3.4.2",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.6.3",
        "vite": "^5.4.12",
        "vitest": "^2.1.9"
    }
}
